lement name="extensions" type="extension-type" minOccurs="0"/>		</xs:sequence>		<xs:attribute name="id" type="xs:string" use="optional"/>		<xs:attribute name="type" type="xs:string" use="required"/>		<!-- add an attribute to allow for the validation of content using xi:includes -->		<xs:attribute ref="xml:base" use="optional"/>	</xs:complexType></xs:schema>